        }

    .home {
    position: relative;
    width: 100%;
    justify-content: space-between;
    height: 100vh;
    background-size: cover;
    background-repeat: no-repeat;
    background: #001b29;
    background-position: center;
    display: flex;
    align-items: center;
    padding: 70px 10% 0;
    }

    .home-contact {
    max-width: 640px;
    }

    .home-contact h3 {
